      "source": [
        "year = int(input('Enter year to check'))  \n",
        "if year % 100 == 0:\n",
        "   if year % 400 == 0:\n",
        "     print('The year',year,'is a leap year')\n",
        "\n",
        "elif year % 4==0:\n",
        "  print('The year',year,'is a leap year')     \n",
        "\n",
        "else:\n",
        "  print('The year',year,'is NOT a leap year')  \n"
      ],
